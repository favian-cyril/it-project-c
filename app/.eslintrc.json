{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "globals": {
        "React": false
    },
    "rules": {
        "semi": 0,
        "comma-dangle": 0,
        "react/jsx-space-before-closing": [1, "never"],
        "no-param-reassign": 0,
        "no-use-before-define": 0,
        "react/jsx-filename-extension": 0,
        "react/no-unused-prop-types": 0,
        "react/self-closing-comp": 0,
        "import/extensions": 0
    },
    "root": true
}